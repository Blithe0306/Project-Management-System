<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<include file="struts-base.xml" />

	<package name="webapp-admin" extends="webapp-base">
		<!-- AdminUserAction -->
		<action name="adminUser" class="admUserAction">
			<result name="success">/manager/admin/user/list.jsp</result>
			<result name="find">/manager/admin/user/add.jsp</result>
			<result name="view">/manager/admin/user/view.jsp</result>
			<result name="repassword">/manager/main/repassword.jsp</result>
			<result name="change_creater_user">
				/manager/admin/user/change_creater_user.jsp
			</result>
			<result name="base_find">
				/manager/admin/user/edit_baseinfo.jsp
			</result>
			<result name="resetpwd">/install/activate.jsp</result>
		</action>

		<!-- RoleInfoAction -->
		<action name="adminRole" class="roleInfoAction">
			<result name="success">/manager/admin/role/list.jsp</result>
			<result name="find">/manager/admin/role/add.jsp</result>
			<result name="sel_role">/manager/admin/user/sel_role.jsp</result>
		    <result name="relogin"  >/login/temp.jsp</result>
		    <result name="change_creater_role">/manager/admin/role/change_creater_role.jsp</result>
		</action>
		
		<!-- adminBindAction -->
		<action name="adminBind" class="adminBindAction">
			<result name="success">
				/manager/user/userinfo/m_m_bind.jsp
			</result>
		</action>		
	</package>

</struts>