<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<include file="struts-base.xml" />

	<package name="webapp-logs" extends="webapp-base">

		<!-- AdminLogAction -->
		<action name="adminLog" class="adminLogAction">
			<result name="success">
				/manager/logs/adminlog/list.jsp
			</result>
			<result name="view">
				/manager/logs/adminlog/view.jsp
			</result>
		</action>

		<!-- UserLogAction -->
		<action name="userLog" class="userLogAction">
			<result name="success">
				/manager/logs/userlog/list.jsp
			</result>
			<result name="view">
				/manager/logs/userlog/view.jsp
			</result>
		</action>

	</package>

</struts>