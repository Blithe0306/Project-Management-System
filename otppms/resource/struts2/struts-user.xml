<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<include file="struts-base.xml" />

	<package name="webapp-user" extends="webapp-base">
		<!-- UserInfoAction -->
		<action name="userInfo" class="uInfoAction">
			<result name="success">
				/manager/user/userinfo/list.jsp
			</result>
			<result name="find">/manager/user/userinfo/edit.jsp</result>
			<result name="view">/manager/user/userinfo/view.jsp</result>
			<result name="change_token_init">
				/manager/user/userinfo/m_m_changeTkn.jsp
			</result>
			<result name="pwd">
				/manager/user/userinfo/bind_pin.jsp
			</result>
			<result name="changeOrgunit">
				/manager/user/userinfo/changeOrgunit.jsp
			</result>
		</action>

		<!-- UserBindAction -->
		<action name="userBind" class="uBindAction">
			<result name="success">
				/manager/user/userinfo/m_m_bind.jsp
			</result>
		</action>
		
		<!-- UserChangeAction -->
		<action name="userChange" class="uChangeAction">
			<result name="success">
				/manager/user/userinfo/m_m_changeOrg.jsp
			</result>
		</action>

		<!-- UserImportAction -->
		<action name="userImport" class="uImportAction">
			<result name="success">
				/manager/user/userinfo/import_user.jsp
			</result>
		</action>

		<!-- UserExportAction -->
		<action name="userExport" class="uExportAction">
			<result name="success">
				/manager/user/userinfo/export_user.jsp
			</result>
		</action>

		<!-- UserUnBindAction -->
		<action name="userUnBind" class="uUnBindAction">
			<result name="success">
				/manager/user/userinfo/m_m_unbind.jsp
			</result>
		</action>

		<!-- Add actions here -->
	</package>

	<!-- Add packages here -->

</struts>