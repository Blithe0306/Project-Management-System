<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-lazy-init="true">

	<!-- User Token -->
	<bean id="userTokenServ" parent="_baseTransactionProxy">
		<property name="target">
			<bean
				class="com.ft.otp.manager.user_token.service.impl.UserTokenServImpl">
				<property name="userTokenDao">
					<bean
						class="com.ft.otp.manager.user_token.dao.impl.UserTokenDaoImpl"
						parent="_baseSqlMapClientTarget">
					</bean>
				</property>
			</bean>
		</property>
	</bean>

</beans>
