<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-lazy-init="true">

	<!-- User Info -->
	<bean id="userInfoServ" parent="_baseTransactionProxy">
		<property name="target">
			<bean
				class="com.ft.otp.manager.user.userinfo.service.impl.UserInfoServImpl">
				<property name="userInfoDao">
					<bean
						class="com.ft.otp.manager.user.userinfo.dao.impl.UserInfoDaoImpl"
						parent="_baseSqlMapClientTarget">
					</bean>
				</property>
			</bean>
		</property>
	</bean>

</beans>
