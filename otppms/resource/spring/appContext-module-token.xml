<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-lazy-init="true">

	<!-- token  -->
	<bean id="tokenServ" parent="_baseTransactionProxy">
		<property name="target">
			<bean
				class="com.ft.otp.manager.token.service.impl.TokenServImpl">
				<property name="tokenDao">
					<bean
						class="com.ft.otp.manager.token.dao.impl.TokenDaoImpl"
						parent="_baseSqlMapClientTarget">
					</bean>
				</property>
			</bean>
		</property>
	</bean>

	<!--distmanager info -->
	<bean id="distManagerServ" parent="_baseTransactionProxy">
		<property name="target">
			<bean
				class="com.ft.otp.manager.token.distmanager.service.impl.DistManagerServImpl">
				<property name="distManagerDao">
					<bean
						class="com.ft.otp.manager.token.distmanager.dao.impl.DistManagerDaoImpl"
						parent="_baseSqlMapClientTarget">
					</bean>
				</property>
			</bean>
		</property>
	</bean>

</beans>
