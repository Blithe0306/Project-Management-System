<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-lazy-init="true">

	<!-- agentConf -->
	<bean id="agentConfServ" parent="_baseTransactionProxy">
		<property name="target">
			<bean
				class="com.ft.otp.manager.authmgr.agentconf.service.impl.AgentConfServImpl">
				<property name="agentConfDao">
					<bean
						class="com.ft.otp.manager.authmgr.agentconf.dao.impl.AgentConfDaoImpl"
						parent="_baseSqlMapClientTarget">
					</bean>
				</property>
			</bean>
		</property>
	</bean>
	
	<!-- auth agent -->
	<bean id="agentServ" parent="_baseTransactionProxy">
		<property name="target">
			<bean
				class="com.ft.otp.manager.authmgr.agent.service.impl.AgentServImpl">
				<property name="agentDao">
					<bean
						class="com.ft.otp.manager.authmgr.agent.dao.impl.AgentDaoImpl"
						parent="_baseSqlMapClientTarget">
					</bean>
				</property>
			</bean>
		</property>
	</bean>

	<!-- auth server -->
	<bean id="serverServ" parent="_baseTransactionProxy">
		<property name="target">
			<bean
				class="com.ft.otp.manager.authmgr.server.service.impl.ServerServImpl">
				<property name="serverDao">
					<bean
						class="com.ft.otp.manager.authmgr.server.dao.impl.ServerDaoImpl"
						parent="_baseSqlMapClientTarget">
					</bean>
				</property>
			</bean>
		</property>
	</bean>

	<!-- auth agnet-server -->
	<bean id="agentServerServ" parent="_baseTransactionProxy">
		<property name="target">
			<bean
				class="com.ft.otp.manager.authmgr.agentserver.service.impl.AgentServerServImpl">
				<property name="agentServerDao">
					<bean
						class="com.ft.otp.manager.authmgr.agentserver.dao.impl.AgentServerDaoImpl"
						parent="_baseSqlMapClientTarget">
					</bean>
				</property>
			</bean>
		</property>
	</bean>
	
	<!-- auth backend -->
	<bean id="backendServ" parent="_baseTransactionProxy">
		<property name="target">
			<bean
				class="com.ft.otp.manager.authmgr.backend.service.impl.BackendServImpl">
				<property name="backendDao">
					<bean
						class="com.ft.otp.manager.authmgr.backend.dao.impl.BackendDaoImpl"
						parent="_baseSqlMapClientTarget">
					</bean>
				</property>
			</bean>
		</property>
	</bean>

</beans>
