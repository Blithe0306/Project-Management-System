<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<include file="struts-base.xml" />

	<package name="webapp-prj" extends="webapp-base">
		<action name="projectAction" class="projectAction">
			<result name="success">
				/manager/project/list.jsp
			</result>
			<result name="find">/manager/project/add.jsp</result>
			<result name="view">/manager/project/view.jsp</result>
			<result name="goPrjInfoAdd">/manager/prjinfo/add.jsp</result>
			<result name="goSummayAdd">/manager/project/view_summary.jsp</result>
		</action>
	</package>
</struts>