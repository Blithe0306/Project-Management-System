<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<include file="struts-base.xml" />

	<package name="webapp-orgunit" extends="webapp-base">

		<!-- DomainInfoAction -->
		<action name="domainInfo" class="domainInfoAction">
			<result name="success">
				/manager/orgunit/domain/list.jsp
			</result>
			<result name="find">/manager/orgunit/domain/add.jsp</result>
		</action>

		<!-- OrgunitInfoAction -->
		<action name="orgunitInfo" class="orgunitInfoAction">
			<result name="find">
				/manager/orgunit/orgunit/add.jsp
			</result>
			<result name="domainView">
				/manager/orgunit/domain/view.jsp
			</result>
			<result name="orgunitView">
				/manager/orgunit/orgunit/view_1.jsp
			</result>
			<result name="move">
				/manager/orgunit/orgunit/moveOrg.jsp
			</result>
		</action>

		<!-- Add actions here -->
	</package>

	<!-- Add packages here -->

</struts>