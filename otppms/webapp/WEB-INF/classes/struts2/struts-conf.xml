<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<include file="struts-base.xml" />
	<package name="webapp-conf" extends="webapp-base">
	
		<!-- common -->
		<action name="commonAction" class="commConfAction">
			<result name="success">
				/manager/confinfo/config/commonconfig.jsp
			</result>
		</action>
		
		<!-- auth -->
		<action name="authConfAction" class="authConfAction">
			<result name="success">
				/manager/confinfo/config/authconfig.jsp
			</result>
			<result name="initpeap">
				/manager/confinfo/config/peapconfig.jsp
			</result>
		</action>
		
		<!-- user -->
		<action name="userConfAction" class="userConfAction">
			<result name="utknconf">
				/manager/confinfo/config/utknbindconfig.jsp
			</result>
			<result name="udefpwdconf">
				/manager/confinfo/config/userdefpwdconfig.jsp
			</result>
		</action>
		
		<!-- token -->
		<action name="tokenConfAction" class="tokenConfAction">
			<result name="softtkn">
				/manager/confinfo/config/softtknconfig.jsp
			</result>
			<result name="mobiletkn">
				/manager/confinfo/config/mobiletknconfig.jsp
			</result>
			<result name="smstkn">
				/manager/confinfo/config/smstknconfig.jsp
			</result>
			<result name="emeypin">
				/manager/confinfo/config/emeypinconfig.jsp
			</result>
		</action>

		<!-- center -->
		<action name="center" class="centerAction">
			<result name="center">
				/manager/confinfo/config/centerconfig.jsp
			</result>
			<result name="authser">
				/manager/confinfo/config/authserselconfig.jsp
			</result>
		</action>
		
		<!-- otpServerAction -->
		<action name="otpServerAction" class="otpServerAction">
			<result name="success">
				/manager/confinfo/config/otpserconfig.jsp
			</result>
		</action>

		<!-- DBConfInfo -->
		<action name="database" class="dbConfAction">
			<result name="success">
				/manager/confinfo/config/dbconfig.jsp
			</result>
		</action>

		<!-- PortalInfo -->
		<action name="portal" class="portalAction">
			<result name="success">
				/manager/confinfo/portal/portalconfig.jsp
			</result>
			<result name="initpwd">
				/manager/confinfo/portal/logininitpwdconfig.jsp
			</result>
		</action>
		
		<!-- Monitorconf -->
		<action name="monitorconf" class="monitorConfAction">
			<result name="success">
				/manager/confinfo/config/monitorconfig.jsp
			</result>
		</action>

		<!-- accessConAction -->
		<action name="access" class="accessConAction">
			<result name="success">
				/manager/confinfo/access/accesscontrol.jsp
			</result>
		</action>

		<!-- emailInfo -->
		<action name="email" class="emailAction">
			<result name="add">/manager/confinfo/email/add.jsp</result>
			<result name="initFind">/install/emailpage.jsp</result>
		</action>

		<!-- smsInfo -->
		<action name="smsInfo" class="smsAction">
			<result name="edit">/manager/confinfo/sms/add.jsp</result>
		</action>

		<!-- Radius -->
		<action name="radProfile" class="radProfileAction">
			<result name="edit">
				/manager/confinfo/radius/addprofile.jsp
			</result>
		</action>
		<action name="radAttr" class="radAttrAction">
			<result name="edit">
				/manager/confinfo/radius/editattr.jsp
			</result>
		</action>

		<action name="usConf" class="userSourceAction">
			<result name="success">
				/manager/confinfo/usersource/list.jsp
			</result>
			<result name="usconfig">
				/manager/confinfo/usersource/usersource.jsp
			</result>
			<result name="settiming">
				/manager/confinfo/usersource/timing_update_conf.jsp
			</result>
		</action>

		<!-- treeMeun -->
		<action name="treeAction" class="treeAction"></action>

		<!-- NoticeInfo -->
		<action name="pronotice" class="proNoticeAction">
			<result name="add">
				/manager/confinfo/portal/addnotice.jsp
			</result>
			<result name="view">
				/manager/confinfo/portal/view.jsp
			</result>
		</action>
		
		<!-- monitorconfig -->
		<action name="monitorconfig" class="monitorConfigAction">
			<result name="success">
				/manager/confinfo/config/conmonitorconfig.jsp
			</result>
		</action>
		
		
		<action name="langAction" class="com.ft.otp.common.language.LangAction"></action>
	</package>
</struts>