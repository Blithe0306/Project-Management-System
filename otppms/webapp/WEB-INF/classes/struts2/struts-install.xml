<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<include file="struts-base.xml" />

	<package name="webapp-install" extends="webapp-base">
		<!-- installAction -->
		<action name="install" class="installAction">
			<result name="language">/install/systemInfo.jsp</result>
			<result name="toDbPage">/install/dbpage.jsp</result>
			<result name="toFinish">/install/finish.jsp</result>
		</action>
	</package>
</struts>