<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-lazy-init="true">

	<!-- Customer Info -->
	<bean id="custInfoServ" parent="_baseTransactionProxy">
		<property name="target">
			<bean class="com.ft.otp.manager.customer.service.impl.CustomerInfoServImpl">
				<property name="custInfoDao">
					<bean class="com.ft.otp.manager.customer.dao.impl.CustomerInfoDaoImpl"
						parent="_baseSqlMapClientTarget">
					</bean>
				</property>
			</bean>
		</property>
	</bean>

</beans>
