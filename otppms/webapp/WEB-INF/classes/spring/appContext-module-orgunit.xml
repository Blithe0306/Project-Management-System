<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-lazy-init="true">
	<bean id="domainInfoServ" parent="_baseTransactionProxy">
		<property name="target">
			<bean
				class="com.ft.otp.manager.orgunit.domain.service.impl.DomainInfoServImpl">
				<property name="domainInfoDao">
					<bean
						class="com.ft.otp.manager.orgunit.domain.dao.impl.DomainInfoDaoImpl"
						parent="_baseSqlMapClientTarget">
					</bean>
				</property>
			</bean>
		</property>
	</bean>
	
	<bean id="orgunitInfoServ" parent="_baseTransactionProxy">
		<property name="target">
			<bean
				class="com.ft.otp.manager.orgunit.orgunit.service.impl.OrgunitInfoServImpl">
				<property name="orgunitInfoDao">
					<bean
						class="com.ft.otp.manager.orgunit.orgunit.dao.impl.OrgunitInfoDaoImpl"
						parent="_baseSqlMapClientTarget">
					</bean>
				</property>
			</bean>
		</property>
	</bean>

</beans>
