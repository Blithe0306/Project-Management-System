<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-lazy-init="true">
	<!-- monitor service -->
	<bean id="monitorServ" parent="_baseTransactionProxy">
		<property name="target">
			<bean
				class="com.ft.otp.manager.monitor.service.impl.MonitorServImpl">
				<property name="monitorDao">
					<bean
						class="com.ft.otp.manager.monitor.dao.impl.MonitorDaoImpl"
						parent="_baseSqlMapClientTarget">
					</bean>
				</property>
			</bean>
		</property>
    </bean>
</beans>
