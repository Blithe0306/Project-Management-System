<?xml version="1.0" encoding="UTF-8"?>
<config>	
	<product version="v4.2.0"> <!-- 表的顺序不能打乱；数据还原时依据此顺序执行insert语句 -->
	    <table>
			<name>otppms_domaininfo</name>
			<auto-incremented>domainid</auto-incremented>
			<columns>domainid,domainsn,domainname,descp,createtime</columns>
			<sequence>ftp_domaininfo_id_seq</sequence>
		</table>
		<table>
			<name>otppms_orgunitinfo</name>
			<auto-incremented>orgunitid</auto-incremented>
			<columns>orgunitid,orgunitnumber,orgunitname,parentid,domainid,descp,createtime</columns>
			<sequence>ftp_orgunitinfo_orgunitid_seq</sequence>
		</table>
		<table>
			<name>otppms_rad_profile</name>
			<auto-incremented>id</auto-incremented>
			<columns>id,profilename,descp</columns>
			<sequence>ftp_rad_profile_id_seq</sequence>
		</table>
		
		<table>
			<name>otppms_userinfo</name>
			<columns>userid,realname,localauth,pwd,pwddeath,getpwddeath,initactpwd,initactpwddeath,paperstype,papersnumber,email,address,tel,cellphone,
				     locked,temploginerrcnt,longloginerrcnt,loginlocktime,logincnt,lastlogintime,backendauth,enabled,domainid,orgunitid,
				     radprofileid,createtime
			</columns>
		</table>
		<table>
			<name>otppms_tokenspec</name>
			<columns>specid,tokentype,algid,signsuite,cvssuite,crsuite,atid,otplen,intervaltime,begintime,maxauthcnt,initauthnum,
				     haformat,halen,cardrow,cardcol,rowstart,colstart,updatemode,updatelimit,updateresplen,puk1mode,
				     puk1len,puk1itv,puk2mode,puk2len,puk2itv,maxcounter,syncmode,descp
			</columns>
		</table>
		<table>
			<name>otppms_tokeninfo</name>
			<columns>token,enabled,locked,lost,logout,pubkey,authnum,authmethod,empin,empindeath,expiretime,syncwnd,
				     temploginerrcnt,longloginerrcnt,loginlocktime,driftcount,physicaltype,producttype,
				     specid,domainid,orgunitid,gensmstime,
				     importtime,distributetime,crauthnum,authtime,crauthtime,authotp,crauthotp,crauthdata,
				     cractivecode,pubkeystate,newpubkey,cractivetime,cractivecount,vendorid
			</columns>
		</table>
		<table>
			<name>otppms_tokenext</name>
			<columns>token,pubkeyfactor,phoneudid,activepass,apdeath,apretry,actived,activetime,provtype,exttype</columns>
		</table>
		<table>
			<name>otppms_user_token</name>
			<auto-incremented>bindid</auto-incremented>
			<columns>bindid,userid,token,domainid,bindtime</columns>
			<sequence>ftp_user_token_bindid_seq</sequence>
		</table>
		<table>
			<name>otppms_hostinfo</name>
			<columns>hostipaddr,hostname,confid,priority,licid,descp,ftradiusenabled,protocoltype,authport,syncport,radiusenabled,
					radauthport,httpenabled,httpport,httpsenabled,httpsport,keystorepwd,certificatepwd,keystoreinstance,keystorerootpath,soapenabled,soapport,webservicename
			</columns>
		</table>
		<table>
			<name>otppms_agentconf</name>
			<auto-incremented>id</auto-incremented>
			<columns>id,confname,type,userformat,localprotect,remoteprotect,uacprotect,unboundlogin,descp
			</columns>
			<sequence>ftp_agentconf_id_seq</sequence>
		</table>
		<table>
			<name>otppms_agentinfo</name>
			<columns>agentipaddr,agentname,pubkey,enabled,descp,agenttype,agentconfid,graceperiod</columns>
		</table>
		<table>
			<name>otppms_agent_host</name>
			<columns>agentipaddr,hostipaddr</columns>
		</table>
		<table>
			<name>otppms_licinfo</name>	
			<columns>licid,licinfo,lictype,issuer,customerid,licstate,licupdatetime</columns>
		</table>
		
		<table>
			<name>otppms_loginfo</name>
			<columns>id,userid,token,serverip,clientip,actionid,actionresult,logtime,logcontent,hashcode,domainid,
					 domainname,orgunitid,orgunitname,moduletype,vendorid
			</columns>
			<logdata>true</logdata> <!-- 此标签表示是日志数据 -->
		</table>
		<table>
			<name>otppms_admininfo</name>
			<columns>adminid,realname,localauth,pwd,pwdsettime,getpwddeath,locked,temploginerrcnt
					 ,longloginerrcnt,loginlocktime,logintime,logincnt,createuser
					 ,enabled,email,address,tel,cellphone,createtime,descp
			</columns>
		</table>
		<table>
			<name>otppms_admin_orgunit</name>
			<columns>adminid,domainid,orgunitid</columns>
		</table>
		<table>
			<name>otppms_perminfo</name>
			<columns>permcode,permlink,srcname,keymark,descp</columns>
		</table>
		<table>
			<name>otppms_roleinfo</name>
			<auto-incremented>roleid</auto-incremented>
			<columns>roleid,rolename,rolemark,createuser,createtime,modifytime,descp</columns>
			<sequence>ftp_roleinfo_roleid_seq</sequence>
		</table>
		<table>
			<name>otppms_role_perm</name>
			<columns>roleid,permcode</columns>
		</table>
		<table>
			<name>otppms_admin_role</name>
			<columns>adminid,roleid</columns>
		</table>
		<table>
			<name>otppms_admin_log</name>
			<auto-incremented>id</auto-incremented>
			<columns>id,operator,logtime,actionid,actionresult,actionobject,clientip,descp,hashcode</columns>
			<sequence>ftp_admin_log_id_seq</sequence>
			<logdata>true</logdata> <!-- 此标签表示是日志数据 -->
		</table>
		<table>
			<name>otppms_taskinfo</name>
			<columns>taskname,sourceid,sourcetype,taskmode1,taskmode2,taskminute,taskhour,taskday,taskmonth,taskweek,enabled,taskid,descp</columns>
		</table>
		<table>
			<name>otppms_configinfo</name>
			<auto-incremented>id</auto-incremented>
			<columns>id,confname,conftype,confvalue,parentid,descp</columns>
			<sequence>ftp_configinfo_id_seq</sequence>
		</table>
		<table>
			<name>otppms_usersource</name>
			<auto-incremented>id</auto-incremented>
			<columns>id,sourcename,sourcetype,dbtype,serveraddr,port,username,pwd,dbname
					 ,dbtablename,filter,basedn,rootdn,dominodriver,namesfile,timeout,isupdateou,upinvaliduser,domainid,orgunitid,localusermark,issyncuserinfo,descp
			</columns>
			<sequence>ftp_usersource_id_seq</sequence>
		</table>
		
		<table>
			<name>otppms_usersource_attr</name>
			<columns>sourceid,localuserattr,sourceuserattr
			</columns>
		</table>
		
		<table>
			<name>otppms_emailinfo</name>
			<auto-incremented>id</auto-incremented>
			<columns>id,servname,account,pwd,sender,host,port,validated,isdefault,descp
			</columns>
			<sequence>ftp_emailinfo_id_seq</sequence>
		</table>
		<table>
			<name>otppms_smsinfo</name>
			<auto-incremented>id</auto-incremented>
			<columns>id,smsname,sendtype,host,username,pwd,accountattr,passwdattr,phoneattr,messageattr
						,paramannex,sendresult,enabled,priority,retrysend,descp
			</columns>
			<sequence>ftp_smsinfo_id_seq</sequence>
		</table>
		<table>
			<name>otppms_attrinfo</name>
			<columns>attrtype,attrid,attrname,attrvalue,valuetype,profileid,vendorid
			</columns>
		</table>
		<table>
			<name>otppms_trustipinfo</name>
			<auto-incremented>id</auto-incremented>
			<columns>id,systype,clientapptype,startip,endip,expiretime,updatetime,enabled,clientappport
					 ,clientservpath
			</columns>
			<sequence>ftp_trustipinfo_id_seq</sequence>
		</table>
		<table>
			<name>otppms_portal_notice</name>
			<auto-incremented>id</auto-incremented>
			<columns>id,systype,createuser,createtime,expiretime,content,title
			</columns>
		</table>
		<table>
			<name>otppms_backend</name>
			<auto-incremented>id</auto-incremented>
			<columns>id,backendtype,backendname,host,sparehost,port,priority,basedn,filter,pubkey
					 ,timeout,retrycnt,usernamerule,delimiter,enabled,policy,domainid
			</columns>
			<sequence>ftp_backend_id_seq</sequence>
		</table>
	</product>
	
</config>